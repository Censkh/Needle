{
  "name": "needle-inject",
  "version": "1.0.2",
  "description": "Seamless dependency injection for Typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "test": "./node_modules/.bin/jest",
    "prepare": "npm run build && npm run test && npm version patch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/09jwater/Needle.git"
  },
  "keywords": [
    "dependency injection"
  ],
  "author": "James Waterhouse",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/09jwater/Needle/issues"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "homepage": "https://gitlab.com/09jwater/Needle#README",
  "devDependencies": {
    "@types/jest": "^23.0.2",
    "@types/node": "^10.3.2",
    "jest": "^23.1.0",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.1"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.12"
  }
}
